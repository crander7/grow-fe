{
  "name": "grow-fe",
  "version": "0.1.1",
  "private": true,
  "proxy": "http://localhost:3001",
  "scripts": {
    "dev": "concurrently \"yarn server\" \"yarn client\"",
    "server": "NODE_ENV=development nodemon index.js",
    "client": "react-scripts start",
    "start-local": "NODE_ENV=production node index.js",
    "start": "NODE_ENV=production pm2 start index.js --name grow-fe",
    "show-all": "pm2 list",
    "show": "pm2 show grow-fe",
    "stop": "pm2 stop grow-fe",
    "delete": "pm2 delete grow-fe",
    "end": "yarn stop && yarn delete",
    "restart": "pm2 restart grow-fe",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-scripts": "2.1.8",
    "request": "^2.88.0"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "node-sass": "^4.11.0",
    "nodemon": "^1.18.10",
    "pm2": "^3.3.1"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
