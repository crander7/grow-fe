{
  "name": "grow-fe",
  "version": "0.1.1",
  "private": true,
  "proxy": "http://localhost:3001",
  "scripts": {
    "dev": "concurrently \"yarn server\" \"yarn client\"",
    "server": "NODE_ENV=development nodemon index.js",
    "client": "react-scripts start",
    "start-local": "NODE_ENV=production node index.js",
    "start": "NODE_ENV=production pm2 start index.js --name grow-fe",
    "show-all": "pm2 list",
    "show": "pm2 show grow-fe",
    "stop": "pm2 stop grow-fe",
    "delete": "pm2 delete grow-fe",
    "end": "yarn stop && yarn delete",
    "restart": "pm2 restart grow-fe",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "pm2": "^3.3.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-scripts": "2.1.8",
    "request": "^2.88.0"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "babel-eslint": "9.0.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-named-asset-import": "^0.3.1",
    "concurrently": "^4.1.0",
    "eslint": "^5.15.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "node-sass": "^4.11.0",
    "nodemon": "^1.18.10",
    "webpack": "^4.29.6"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
